    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}",
            "env": { "mongodbURI":"mongodb://localhost:27017",
                   "compressor":"zstd",
                   "megabytesToRead": 10,
                   "recordBatchSize":100}   
        },
        {
            "type": "java",
            "name": "Launch App",
            "request": "launch",
            "mainClass": "com.mongodb.java.bootstrap.App",
            "projectName": "java-bootstrap",
            "env":{"mongodb_uri":"mongodb://localhost:27017",
                "compressor":"zlib",
                "megabytesToRead": 10,
                "recordBatchSize":100}      
        }
    ]
}